<plugin
	id="test.projecttojs"
	name="Generate Javascript"
	description="Generate Javascript files from elements"
	provider="Visual Paradigm"
	class="test.projecttojs.ProjectToJs">
    <runtime>
		<library path="lib/jsonsimple.jar" relativePath="true" />
		<library path="lib/commons-lang3-3.4.jar" relativePath="true" />
		<library path="lib/guava-19.0.jar" relativePath="true" />
		<library path="lib/commons-io-2.5.jar" relativePath="true" />
	</runtime>  
	<actionSets>
		<actionSet id="test.projecttojs.actionset">
			<action 
				id="test.projecttojs.ProjectToJsActionController"
				actionType="generalAction"
				label="Generate Javascript"
				tooltip="Generate Javascript"
				icon="icons/JS.png"
				style="normal"
				menuPath="Tools/Report">
				<actionController 
class="test.projecttojs.actions.ProjectToJsActionController"/>
			</action>
		</actionSet>
	</actionSets>
	
</plugin>